# LocationFinder 插件配置示例 v3.0
# 多源融合定位系统

# 基础配置
# 主要API端点（可选，默认使用ipapi.co）
API_ENDPOINT=https://ipapi.co/json/

# API请求超时时间（毫秒，默认15000）
API_TIMEOUT=15000

# 多源定位配置
# 是否启用多源数据融合（推荐启用，默认false）
ENABLE_MULTI_SOURCE=true

# 最小置信度阈值（0-1，默认0.6）
MIN_CONFIDENCE_THRESHOLD=0.6

# 最大API调用数量（默认3）
MAX_API_CALLS=3

# 缓存配置
# 是否启用缓存（默认true）
ENABLE_CACHE=true

# 缓存过期时间（毫秒，默认1小时）
CACHE_TTL=3600000

# 缓存文件路径（默认location_cache.json）
CACHE_FILE=location_cache.json

# 地图服务API密钥
# 高德地图API（国内服务，支持WiFi、GPS定位）
AMAP_API_KEY=e89469983d5dbc8dddc5466b4f72a6cd

# Google Maps Geolocation API（支持WiFi、蓝牙、基站定位）
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# 百度地图API（国内服务，支持WiFi、GPS定位）
BAIDU_MAPS_API_KEY=your_baidu_maps_api_key_here

# IP定位服务密钥
# IPinfo.io API密钥（可选）
IPINFO_TOKEN=your_ipinfo_token_here

# MaxMind GeoIP2 API密钥（商业级）
MAXMIND_API_KEY=your_maxmind_api_key_here

# 性能监控
# 是否启用性能监控（默认true）
ENABLE_PERFORMANCE_MONITORING=true

# 性能日志文件路径（默认performance.log）
PERFORMANCE_LOG_FILE=performance.log